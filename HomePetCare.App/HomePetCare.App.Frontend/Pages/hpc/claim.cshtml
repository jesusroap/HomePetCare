@page
@model HomePetCare.App.Frontend.Pages.claimModel
@{
    ViewData["Title"] = "Claims";
}

<section id="details" class="wow fadeInUp">
    <h1>@ViewData["Title"]</h1>
    @if (User.Identity.IsAuthenticated)
    {
        <table class="table table-sm">
            @foreach (var claim in User.Claims)
            {
                <tr>
                    <td>@claim.Type</td>
                    <td>@claim.Value</td>
                </tr>
            }
        </table>
    }
</section>